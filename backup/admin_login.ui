<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Frame" id="Main_frame">
    <property name="background">#1a543e</property>
    <property name="height">500</property>
    <property name="width">800</property>
    <layout manager="pack">
      <property name="expand">true</property>
      <property name="fill">both</property>
      <property name="propagate">False</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Label" id="image_label_main">
        <property name="image">image.png</property>
        <property name="text" translatable="yes">label2</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.02</property>
          <property name="rely">0.34</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="mm_label_main">
        <property name="background">#1a543e</property>
        <property name="compound">bottom</property>
        <property name="font">{ARVO} 14 {italic}</property>
        <property name="foreground">white</property>
        <property name="text" translatable="yes">Message Management</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.21</property>
          <property name="rely">0.4</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
